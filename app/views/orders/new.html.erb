<h1>Order</h1>
<%= form_with model: @order, method: :post, url: '/users/:user_id/orders'  do |f|%>
 <%= f.label 'Select pizza(s)'%> <br>
 <%= f.collection_select(:pizza_id, Pizza.all, :id, :name, {}, {multiple: true})%> <br>
 <br><%= f.label 'Special request(s)'%> <br>
 <%= f.text_area :note %> <br>
 
 <%= f.submit%>
<% end %>